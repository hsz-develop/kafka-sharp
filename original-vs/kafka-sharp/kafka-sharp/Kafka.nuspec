<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>kafka-sharp</id>
        <version>$version$</version>
        <summary>A C# Kafka driver</summary>
        <description>A .NET implementation of the Apache Kafka client side protocol geared toward performance (both throughput and memory wise). It is especially suited for scenarios where applications are streaming a large number of messages across a fair number of topics.</description>
        <authors>Criteo</authors>
        <copyright>$copyright$</copyright>
        <projectUrl>https://github.com/criteo/kafka-sharp</projectUrl>
        <dependencies>
            <dependency id="Snappy.NET" version="1.1.1.8" />
            <dependency id="System.Reactive.Core" version="3.1.1" />
            <dependency id="System.Reactive.Interfaces" version="3.1.1" />
            <dependency id="System.Reactive.Linq" version="3.1.1" />
            <dependency id="System.Threading.Tasks.Dataflow" version="4.7.0" />
        </dependencies>
    </metadata>
</package>
